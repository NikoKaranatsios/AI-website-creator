{
  "pairs": [
    {
      "prompt": "Create a blue button that says Click Me",
      "component": {
        "type": "button",
        "props": {
          "className": "bg-blue-500 hover:bg-blue-600 text-white px-4 py-2 rounded",
          "onClick": "handleClick"
        },
        "children": ["Click Me"]
      }
    },
    {
      "prompt": "Make a heading that says Welcome to my website",
      "component": {
        "type": "h1",
        "props": {
          "className": "text-3xl font-bold text-gray-900"
        },
        "children": ["Welcome to my website"]
      }
    },
    {
      "prompt": "Create a card with an image and title",
      "component": {
        "type": "div",
        "props": {
          "className": "bg-white rounded-lg shadow-md overflow-hidden"
        },
        "children": [
          {
            "type": "img",
            "props": {
              "className": "w-full h-48 object-cover",
              "src": "https://images.unsplash.com/photo-1506744038136-46273834b3fb",
              "alt": "Card image"
            }
          },
          {
            "type": "div",
            "props": {
              "className": "p-4"
            },
            "children": [
              {
                "type": "h2",
                "props": {
                  "className": "text-xl font-semibold"
                },
                "children": ["Beautiful Landscape"]
              }
            ]
          }
        ]
      }
    },
    {
      "prompt": "Create a red button that says Submit",
      "component": {
        "type": "button",
        "props": {
          "className": "bg-red-500 hover:bg-red-600 text-white px-4 py-2 rounded",
          "onClick": "handleSubmit"
        },
        "children": ["Submit"]
      }
    },
    {
      "prompt": "Make a paragraph with some descriptive text",
      "component": {
        "type": "p",
        "props": {
          "className": "text-gray-700 leading-relaxed"
        },
        "children": [
          "This is a paragraph with descriptive text that provides information about something."
        ]
      }
    },
    {
      "prompt": "Create an input field with placeholder text Enter your name",
      "component": {
        "type": "input",
        "props": {
          "className": "border rounded px-4 py-2 w-full",
          "type": "text",
          "placeholder": "Enter your name"
        }
      }
    },
    {
      "prompt": "Make a navigation bar with three links: Home, About, Contact",
      "component": {
        "type": "nav",
        "props": {
          "className": "bg-gray-800 p-4 flex justify-between items-center text-white"
        },
        "children": [
          {
            "type": "a",
            "props": {
              "className": "hover:text-gray-400",
              "href": "/home"
            },
            "children": ["Home"]
          },
          {
            "type": "a",
            "props": {
              "className": "hover:text-gray-400",
              "href": "/about"
            },
            "children": ["About"]
          },
          {
            "type": "a",
            "props": {
              "className": "hover:text-gray-400",
              "href": "/contact"
            },
            "children": ["Contact"]
          }
        ]
      }
    },
    {
      "prompt": "Create a footer with text All rights reserved",
      "component": {
        "type": "footer",
        "props": {
          "className": "bg-gray-900 text-white p-4 text-center"
        },
        "children": ["All rights reserved"]
      }
    },
    {
      "prompt": "Create a green button with animation on hover that says Confirm",
      "component": {
        "type": "button",
        "props": {
          "className": "bg-green-500 hover:bg-green-600 hover:scale-105 transform transition duration-200 text-white px-4 py-2 rounded",
          "onClick": "handleConfirm"
        },
        "children": ["Confirm"]
      }
    },
    {
      "prompt": "Make a collapsible FAQ section",
      "component": {
        "type": "div",
        "props": {
          "className": "w-full max-w-md mx-auto"
        },
        "children": [
          {
            "type": "div",
            "props": {
              "className": "border-b py-2 cursor-pointer",
              "onClick": "toggleFaq"
            },
            "children": [
              {
                "type": "h3",
                "props": {
                  "className": "text-lg font-semibold"
                },
                "children": ["What is your return policy?"]
              }
            ]
          },
          {
            "type": "div",
            "props": {
              "className": "hidden",
              "id": "faq-content"
            },
            "children": [
              {
                "type": "p",
                "props": {
                  "className": "text-gray-600 mt-2"
                },
                "children": ["Our return policy is 30 days with a full refund."]
              }
            ]
          }
        ]
      }
    },
    {
      "prompt": "Create a modal with an animated fade-in effect",
      "component": {
        "type": "div",
        "props": {
          "className": "fixed inset-0 bg-gray-900 bg-opacity-50 flex justify-center items-center",
          "id": "modal"
        },
        "children": [
          {
            "type": "div",
            "props": {
              "className": "bg-white p-6 rounded-lg shadow-lg transform transition-opacity duration-300 opacity-0 fade-in"
            },
            "children": [
              {
                "type": "h2",
                "props": {
                  "className": "text-xl font-bold"
                },
                "children": ["Modal Title"]
              },
              {
                "type": "p",
                "props": {
                  "className": "text-gray-600 mt-2"
                },
                "children": ["This is the modal content."]
              },
              {
                "type": "button",
                "props": {
                  "className": "mt-4 bg-blue-500 hover:bg-blue-600 text-white px-4 py-2 rounded",
                  "onClick": "closeModal"
                },
                "children": ["Close"]
              }
            ]
          }
        ]
      }
    },
    {
      "prompt": "Make a spinning loading indicator",
      "component": {
        "type": "div",
        "props": {
          "className": "w-16 h-16 border-4 border-blue-500 border-dashed rounded-full animate-spin"
        }
      }
    },
    {
      "prompt": "Create an animated card flip component",
      "component": {
        "type": "div",
        "props": {
          "className": "relative w-64 h-96",
          "onClick": "flipCard"
        },
        "children": [
          {
            "type": "div",
            "props": {
              "className": "absolute inset-0 bg-blue-500 text-white flex justify-center items-center rounded-lg transform transition-transform duration-500 card-front"
            },
            "children": ["Front of the Card"]
          },
          {
            "type": "div",
            "props": {
              "className": "absolute inset-0 bg-green-500 text-white flex justify-center items-center rounded-lg transform rotate-y-180 transition-transform duration-500 card-back"
            },
            "children": ["Back of the Card"]
          }
        ]
      }
    },
    {
      "prompt": "Build a progress bar with animation",
      "component": {
        "type": "div",
        "props": {
          "className": "w-full bg-gray-300 rounded-full h-4"
        },
        "children": [
          {
            "type": "div",
            "props": {
              "className": "bg-blue-500 h-4 rounded-full animate-progress",
              "style": {
                "width": "0%"
              },
              "id": "progress-bar"
            }
          }
        ]
      }
    }
  ]
}
